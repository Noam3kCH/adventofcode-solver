  },
  "dependencies": {
    "inquirer": "^4.0.1",
    "js-combinatorics": "^0.5.0",
    "node-fetch": "^1.3.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "haste-cli": "^0.1.16",
    "haste-preset-yoshi": "0.0.11"
  },
  "haste": {
    "preset": "haste-preset-yoshi"
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ]
  },
  "eslintConfig": {
    "extends": "wix/esnext",
    "globals": {
      "itHeavy": false,
      "describeHeavy": false
