"use strict";

var _day = require("./day08.js");

var _readInput = _interopRequireDefault(require("../utils/read-input"));

function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { default: obj }; }

const meta = Object.freeze({
  url: require('url').pathToFileURL(__filename)
});
const input = (0, _readInput.default)(meta.url);
describe('day08 2016', () => {
  describe('part1', () => {
    it('should work for part 1 examples', () => {
      expect((0, _day.part1)(`rect 3x2
rotate column x=1 by 1
rotate row y=0 by 4
rotate column x=1 by 1`, 7, 3)).toEqual(6);
    });
    it('should work for part 1 input', () => {
      expect((0, _day.part1)(input)).toEqual(128);
    });
  });
  describe('part2', () => {
    it('should work for part 2 examples', () => {
      expect((0, _day.part2)(`rect 3x2
rotate column x=1 by 1
rotate row y=0 by 4
rotate column x=1 by 1`, 7, 3)).toEqual(['', '.#..#.#', '#.#....', '.#.....'].join('\n'));
    });
    it('should work for part 2 input', () => {
      expect((0, _day.part2)(input)).toEqual(['', '####..##...##..###...##..###..#..#.#...#.##...##..', '#....#..#.#..#.#..#.#..#.#..#.#..#.#...##..#.#..#.', '###..#..#.#..#.#..#.#....#..#.####..#.#.#..#.#..#.', '#....#..#.####.###..#.##.###..#..#...#..####.#..#.', '#....#..#.#..#.#.#..#..#.#....#..#...#..#..#.#..#.', '####..##..#..#.#..#..###.#....#..#...#..#..#..##..'].join('\n')); //EOARGPHYAO
    });
  });
});
