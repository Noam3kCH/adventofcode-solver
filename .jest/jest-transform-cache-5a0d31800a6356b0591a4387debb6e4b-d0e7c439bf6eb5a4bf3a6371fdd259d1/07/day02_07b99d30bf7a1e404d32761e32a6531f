const part1 = input => input.split('\n').map(line => {
  const numbers = line.split(/\s+/).map(x => parseInt(x)).sort((a, b) => a - b);
  return numbers.pop() - numbers.shift();
}).reduce((sum, x) => sum + x, 0);

exports.part1 = part1;

const part2 = input => input.split('\n').map(line => {
  const numbers = line.split(/\s+/).map(x => parseInt(x)).sort((a, b) => a - b);

  const divisible = a => b => a % b === 0;

  let a, b;

  while (numbers.length > 0 && b === undefined) {
    a = numbers.pop();
    b = numbers.find(divisible(a));
  }

  return a / b;
}).reduce((sum, x) => sum + x, 0);

exports.part2 = part2;
