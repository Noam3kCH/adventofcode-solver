function validTriangle(sides) {
  return sides[0] + sides[1] > sides[2] && sides[0] + sides[2] > sides[1] && sides[1] + sides[2] > sides[0];
}

function rotate(sides, index, lines) {
  return [lines[index + -1 * (index % 3)][index % 3], lines[index + 1 + -1 * (index % 3)][index % 3], lines[index + 2 + -1 * (index % 3)][index % 3]];
}

function parse(input) {
  return input.split('\n').map(x => {
    return x.replace(/^\s*/, '').split(/\s+/).map(x => parseInt(x));
  });
}

const part1 = input => parse(input).filter(validTriangle).length;

exports.part1 = part1;

const part2 = input => parse(input).map(rotate).filter(validTriangle).length;

exports.part2 = part2;
