exports.part1 = part1;
exports.part2 = part2;

function part1(input, noun = 12, verb = 2) {
  const ops = input.split(',').map(x => parseInt(x));
  let ip = 0;
  ops[1] = noun;
  ops[2] = verb;

  while (ops[ip] !== 99) {
    if (ops[ip] === 1) {
      ops[ops[ip + 3]] = ops[ops[ip + 1]] + ops[ops[ip + 2]];
    } else if (ops[ip] === 2) {
      ops[ops[ip + 3]] = ops[ops[ip + 1]] * ops[ops[ip + 2]];
    }

    ip += 4;
  }

  return ops[0];
}

function part2(input) {
  for (let noun = 0; noun <= 99; noun++) {
    for (let verb = 0; verb <= 99; verb++) {
      if (part1(input, noun, verb) === 19690720) {
        return noun * 100 + verb;
      }
    }
  }

  return 0;
}
