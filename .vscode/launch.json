{
  "version": "0.2.0",
  "configurations": [{
    "name": "Launch",
    "type": "node",
    "request": "launch",
    "program": "${workspaceRoot}/src/index.js",
    "args": ["2016", "3"],
    "cwd": "${workspaceRoot}",
    "env": {
      "NODE_ENV": "test"
    },
    "runtimeArgs": [
      "--nolazy",
      "--harmony"
    ]
  }, {
    "name": "Launch Mocha",
    "type": "node",
    "request": "launch",
    "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
    "args": ["{test,src}/**/*.spec.js{,x}", "--no-timeouts"],
    "cwd": "${workspaceRoot}",
    "env": {
      "NODE_ENV": "test"
    },
    "runtimeArgs": [
      "--nolazy",
      "--harmony",
      "--require", "${workspaceRoot}/node_modules/wix-node-build/lib/ignore-extensions.js"
    ]
  }]
}